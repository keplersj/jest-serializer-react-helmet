    "react-helmet",
    "unit-testing",
    "snapshot-testing"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keplersj/jest-serializer-react-helmet/issues"
  },
  "homepage": "https://github.com/keplersj/jest-serializer-react-helmet#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
