    "@types/node": "^13.1.8",
    "@types/react": "^16.9.1",
    "@types/react-helmet": "^5.0.9",
    "@types/react-test-renderer": "^16.9.0",
    "eslint": "^6.1.0",
    "eslint-config-starstuff": "^1.4.5",
    "husky": "^4.0.0",
    "jest": "^25.1.0",
    "jest-runner-eslint": "^0.7.4",
    "jest-runner-prettier": "^0.3.1",
    "prettier": "^2.0.0",
    "react": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.9.0",
    "semantic-release": "^17.0.1",
    "ts-jest": "^25.0.0",
